{
	"name": "@gander-tools/visual-regression-testing-system",
	"type": "module",
	"devDependencies": {
		"@playwright/test": "^1.58.0",
		"@types/node": "^25.2.0",
		"tsx": "^4.20.0",
		"typescript": "^5.9.0"
	},
	"engines": {
		"node": ">=24"
	},
	"license": "GPL-3.0-only",
	"private": true,
	"scripts": {
		"visual:init": "tsx src/init-config.ts",
		"visual:generate": "tsx src/generate-visual-baseline.ts",
		"visual:test": "tsx src/run-visual-tests.ts",
		"visual:report": "playwright show-report .visual-regression/report",
		"visual:inspect": "tsx src/inspect-page.ts",
		"typecheck": "tsc --noEmit",
		"check": "npx -y @biomejs/biome check",
		"check:fix": "npx -y @biomejs/biome check --write"
	}
}
